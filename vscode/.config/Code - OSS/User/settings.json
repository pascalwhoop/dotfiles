{
	"cSpell.userWords": [
		"discoverability",
		"gitlab",
		"integratability",
		"kubernetes"
	],
	"cloudcode.enableTelemetry": false,
	"diffEditor.ignoreTrimWhitespace": false,
	"editor.lineNumbers": "relative",
	"emmet.includeLanguages": {
		"javascript": "html"
	},
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	"files.autoSave": "onFocusChange",
	"git.enableSmartCommit": true,
	"gitlens.advanced.telemetry.enabled": false,
	"go.formatTool": "goimports",
	"markdown-toc.orderedList": true,
	"presentationMode.zoomLevel": 2,
	"prettier.printWidth": 100,
	"prettier.proseWrap": "always",
	"telemetry.enableTelemetry": false,
	"terraform.telemetry.enabled": false,
	"vim.textwidth": 100,
	"workbench.colorTheme": "Default Light+",
	"workbench.tree.indent": 13,
	"[markdown]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.wordWrap": "wordWrapColumn",
		"editor.wrappingIndent": "same",
		"editor.wordWrapColumn": 100
	},
	"editor.fontFamily": "'SauceCodePro Nerd Font Mono','Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
	"editor.fontSize": 12,
"window.zoomLevel": 1.5,
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"aceJump.finder.onlyInitialLetter": false,
	"keyboard.dispatch": "keyCode",
	"files.exclude": {
		"**/__pycache__": true,
		"**/.classpath": true,
		"**/.factorypath": true,
		"**/.idea": true,
		"**/.project": true,
		"**/.pytest_cache": true,
		"**/.settings": true,
		"**/.terraform": true
	},
	"git.autofetch": true,
	"workbench.colorCustomizations": {
		//"editor.background": "#383C4a",
		//"list.inactiveSelectionBackground": "#404552",
		//"sideBar.background": "#383C4A",
		//"sideBarSectionHeader.background": "#353945",
		//"activityBar.background": "#383C4A",
		//"tab.inactiveBackground": "#404552",
		//"statusBar.background": "#383C4A"
	},
	"eslint.autoFixOnSave": true,
	"[json]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[yaml]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"cloudcode.gke": {
		"projectIds": [
			"pascalwhoop"
		]
	},
	"[javascriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[html]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"workbench.iconTheme": "vscode-icons",
	"terraform.indexing": {
		"enabled": false,
		"liveIndexing": false,
		"delay": 500,
		"exclude": [
			".terraform/**/*",
			"**/.terraform/**/*"
		]
	},
	"terraform.languageServer": {
		"enabled": true,
		"args": []
	},
	"kite.showWelcomeNotificationOnStartup": false,
	"python.dataScience.askForKernelRestart": false,
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": true
	},
	"java.semanticHighlighting.enabled": true,
	"go.useLanguageServer": true,
	"cSpell.enableFiletypes": [
		"!plaintext"
	],
}
